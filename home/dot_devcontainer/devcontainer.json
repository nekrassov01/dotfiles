{
  "name": "dev-environment",
  "dockerFile": "Dockerfile",
  "remoteUser": "dev-user",
  "workspaceFolder": "/home/dev-user",
  "workspaceMount": "source=${localWorkspaceFolder}/.workspace,target=/home/dev-user,type=bind,consistency=cached",
  "shutdownAction": "none",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.profiles.linux": {
          "bash": {
            "path": "bash",
            "icon": "terminal-bash"
          }
        }
      }
    }
  }
}
